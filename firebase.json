{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "storyteller-450807",
          "appId": "1:416102166155:android:d4795126baf917373dee8e",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "storyteller-450807",
          "configurations": {
            "android": "1:416102166155:android:d4795126baf917373dee8e",
            "web": "1:416102166155:web:7030e554efae8e3e3dee8e"
          }
        }
      }
    }
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    }
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "function": {
          "functionId": "web_search_page",
          "region": "us-central1",
          "codebase": "default"
        }
      },
      {
        "source": "/jokes/**",
        "function": {
          "functionId": "web_search_page",
          "region": "us-central1",
          "codebase": "default"
        }
      },
      {
        "source": "/sitemap.xml",
        "function": {
          "functionId": "web_search_page",
          "region": "us-central1",
          "codebase": "default"
        }
      },
      {
        "source": "/admin",
        "function": {
          "functionId": "web_search_page",
          "region": "us-central1",
          "codebase": "default"
        }
      },
      {
        "source": "/admin/**",
        "function": {
          "functionId": "web_search_page",
          "region": "us-central1",
          "codebase": "default"
        }
      }
    ]
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "py_quill",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "python313"
    }
  ],
  "storage": {
    "rules": "storage.rules"
  }
}
